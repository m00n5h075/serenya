{
  "name": "serenya-backend",
  "version": "1.0.0",
  "description": "AWS serverless backend for Serenya AI Health Agent",
  "main": "infrastructure/app.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "deploy": "npm run build && cdk deploy",
    "deploy:dev": "npm run build && cdk deploy --context environment=dev",
    "deploy:staging": "npm run build && cdk deploy --context environment=staging", 
    "deploy:prod": "npm run build && cdk deploy --context environment=prod",
    "destroy": "cdk destroy",
    "diff": "npm run build && cdk diff",
    "synth": "npm run build && cdk synth",
    "bootstrap": "cdk bootstrap"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.100.0",
    "constructs": "^10.0.0",
    "aws-lambda": "^1.0.7",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^9.0.1",
    "aws-sdk": "^2.1481.0",
    "sharp": "^0.32.6",
    "@anthropic-ai/sdk": "^0.24.2"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@types/aws-lambda": "^8.10.122",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/uuid": "^9.0.5",
    "aws-cdk": "^2.100.0",
    "typescript": "^5.2.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.5",
    "ts-jest": "^29.1.1"
  },
  "keywords": [
    "aws",
    "serverless", 
    "healthcare",
    "hipaa",
    "lambda",
    "cdk"
  ],
  "author": "Serenya Health",
  "license": "PRIVATE"
}